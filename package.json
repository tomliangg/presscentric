    "enzyme-to-json": "3.3.5",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-react": "7.14.2",
    "jest": "24.8.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
